{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost\\SQLEXPRESS;Database=Hood.Web;Trusted_Connection=True;MultipleActiveResultSets=true;",
    "RedisCache": "localhost"
  },
  "LockoutMode": {
    "OverrideToken": "insert-access-override-token"
  },
  "Hood": {
    "SuperAdminEmail": "admin@hooddigital.com",
    "LibraryFolder": "/hood",
    "UI": "Bootstrap4",
    "SeedOnStart": false,
    "LogLevel": "None"
  },
  "Identity": {
    "Password": {
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequiredLength": 6
    },
    "Cookies": {
      "Name": "Hood",
      "ConsentRequired": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}

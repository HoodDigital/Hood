        "Google": {
            "ApiKey": "",
            "EnableMaps": true,
            "EnableGeocoding": true,
            "Recaptcha": {
                "Enabled": true,
                "Key": "",
                "Secret": ""
            }
        },
        "SendGrid": {
            "ApiKey": ""
        },
        "SMTP": {
            "Server": "",
            "Port": "",
            "Username": "",
            "Password": ""
        }
    }
  },
  "Identity": {
    "Auth0": {
      "Domain": "",
      "ClientId": "",
      "ClientSecret": "",
      "SetupRemoteOnIntitialize": false
    },
    "Password": {
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequiredLength": 6
    },
    "Cookies": {
      "Name": "Hood",
      "ConsentRequired": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"

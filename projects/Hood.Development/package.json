{
  "name": "hoodcms",
  "version": "5.0.0-rc1",
  "description": "A fully customisable content management system built in ASP.NET Core 5 & Bootstrap 5.",
  "dependencies": {
    "@popperjs/core": "2.9.2",
    "@simonwep/pickr": "^1.8.0",
    "@types/jquery.slimscroll": "^1.3.31",
    "@types/jquery.validation": "^1.16.6",
    "bootstrap": "5.0.0",
    "bootstrap-icons": "1.5.0",
    "chart.js": "^3.2.1",
    "dropzone": "^5.9.2",
    "jquery": "3.6.0",
    "jquery-slimscroll": "^1.3.8",
    "jquery-toast-plugin": "1.3.2",
    "jquery-validation": "^1.19.3",
    "sweetalert2": "10.16.7",
    "terser": "^5.7.0",
    "tinymce": "^5.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.1",
    "@lopatnov/rollup-plugin-uglify": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/bootstrap": "5.0.13",
    "@types/dropzone": "^5.7.3",
    "@types/google.maps": "^3.45.2",
    "@types/jquery": "3.5.5",
    "@types/jquery-slimscroll": "^1.3.0",
    "@types/jquery-toast-plugin": "1.3.2",
    "@types/randomstring": "^1.1.6",
    "autoprefixer": "10.2.5",
    "css-loader": "5.2.4",
    "cssnano": "5.0.2",
    "eslint": "7.26.0",
    "gulp": "4.0.2",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.3",
    "gulp-imagemin": "7.1.0",
    "gulp-less": "4.0.1",
    "gulp-rename": "2.0.0",
    "gulp-rimraf": "1.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-uglify": "3.0.2",
    "mini-css-extract-plugin": "1.6.0",
    "node-sass": "^6.0.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "npm-watch": "^0.10.0",
    "postcss": "8.2.15",
    "postcss-loader": "5.2.0",
    "rollup": "^2.51.2",
    "rollup-pluginutils": "^2.8.2",
    "sass-loader": "11.1.0",
    "serve": "11.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-twbs-bootstrap": "2.2.0",
    "terser-webpack-plugin": "5.1.1",
    "typescript": "4.2.4"
  },
  "scripts": {
    "build": "npm-run-all scss tsc",
    "build-production": "npm-run-all scss cssnano tsc tsc-production",
    "clean": "gulp clean",
    "copy": "gulp copy",
    "cssnano": "gulp cssnano",
    "package": "npm-run-all clean build-production copy",
    "scss": "gulp scss",
    "scss-lint": "stylelint src/scss/ --fix",
    "tsc": "tsc && npm-run-all tsc-rollup",
    "tsc-production": "tsc --project tsconfig.production.json && npm-run-all tsc-rollup-production",
    "tsc-rollup": "rollup --config --debug",
    "tsc-rollup-production": "rollup --config",
    "watch-scss": "npm-watch scss",
    "watch-tsc": "npm-watch tsc"
  },
  "watch": {
    "scss": {
      "patterns": [
        "src"
      ],
      "extensions": "scss",
      "quiet": false
    },
    "tsc": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HoodDigital/hoodcms.git"
  },
  "keywords": [
    "hood",
    "hoodcms",
    "visual-studio",
    "netcore",
    "net5.0",
    "efcore",
    "ef",
    "dotnet",
    "js",
    "ts",
    "scss"
  ],

{
  "ConnectionStrings": {
      "DefaultConnection": "Server=localhost\\SQLEXPRESS;Database=Hood.Web;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Hood": {
      "SuperAdminEmail": "admin@hooddigital.com",
      "InitializeOnStartup": true,
      "ApplyMigrationsAutomatically": true,
      "LogLevel": "None",
      "BypassCDN": false
  },
  "Identity": {
      "Auth0": {
          "Domain": "",
          "ClientId": "",
          "ClientSecret": "",
          "SetupRemoteOnIntitialize": true
      },
      "Password": {
          "RequireDigit": true,
          "RequireLowercase": true,
          "RequireUppercase": false,
          "RequireNonAlphanumeric": false,
          "RequiredLength": 6
      },
      "Cookies": {
          "Name": "Hood",
          "ConsentRequired": true
      }
  },
  "Logging": {
      "LogLevel": {
          "Default": "Information",
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information"
      }
  }
}
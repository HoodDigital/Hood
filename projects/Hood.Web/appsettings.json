{
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=Hood.Web;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "LockoutMode": {
        "OverrideToken": "insert-access-override-token"
    },
    "Identity": {
        "Password": {
            "RequireDigit": true,
            "RequireLowercase": true,
            "RequireUppercase": false,
            "RequireNonAlphanumeric": false,
            "RequiredLength": 6
        },
        "Cookies": {
        }
    }
}

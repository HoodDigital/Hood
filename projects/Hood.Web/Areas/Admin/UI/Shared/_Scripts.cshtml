<script src="@string.Format("{0}/js/core/alerts.js", Engine.LibraryFolder)" asp-location="Vendors"></script>
<script src="@string.Format("{0}/js/core/forms.js", Engine.LibraryFolder)" asp-location="Vendors"></script>

<script src="~/lib/pickr/dist/pickr.min.js" asp-location="Vendors" asp-bundle="false"></script>

<script src="@string.Format("{0}/js/core/production.js", Engine.LibraryFolder)" asp-location="Vendors"></script>
<script src="@string.Format("{0}/js/core/globals.js", Engine.LibraryFolder)" asp-location="Vendors"></script>


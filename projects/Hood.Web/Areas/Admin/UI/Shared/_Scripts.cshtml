@inject IThemesService _themes
@{
    IntegrationSettings _plugins = Engine.Settings.Integrations;
}

<script src="~/lib/jquery-validate/jquery.validate.min.js" asp-location="Vendors"></script>
<script src="~/lib/jquery-validate-unobtrusive/jquery.validate.unobtrusive.min.js" asp-location="Vendors"></script>

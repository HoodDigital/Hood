<script src="~/lib/bootstrap-datetime/js/bootstrap-datetimepicker.min.js" asp-location="Vendors"></script>
<script src="~/lib/jquery-colorbox/jquery.colorbox-min.js" asp-location="Vendors"></script>
<script src="~/lib/jquery-slimscroll/jquery.slimscroll.min.js" asp-location="Vendors"></script>
<script src="~/lib/sweetalert/dist/sweetalert2.min.js" asp-location="Vendors"></script>
<script src="~/lib/selectize/js/standalone/selectize.min.js" asp-location="Vendors"></script>
<script src="~/lib/dropzone/min/dropzone.min.js" asp-location="Vendors"></script>

<script src="~/lib/tinymce/tinymce.min.js" asp-location="Vendors" asp-bundle="false"></script>

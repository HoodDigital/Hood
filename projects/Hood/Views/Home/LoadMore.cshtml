@model ContentModel
@{
    Layout = null;
}
@if (Model.List.Count() > 0)
{
    foreach (Content content in Model.List)
    {
        switch (Model.ContentType.BaseName)
        {
            case "News":
                await Html.RenderPartialAsync("_NewsCard", content);
                break;
            case "Portfolio":
                await Html.RenderPartialAsync("_PortfolioCard", content);
                break;
        }
    }
    await Html.RenderPartialAsync("_LoadMoreBlock");
}

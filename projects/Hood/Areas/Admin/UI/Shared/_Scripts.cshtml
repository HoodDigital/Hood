@inject IThemesService _themes
@{
    IntegrationSettings _plugins = _settings.GetIntegrationSettings();
}

@Html.RenderScripts(Url, ResourceLocation.BeforeJquery)
@Html.RenderInlineScripts(Url, ResourceLocation.BeforeJquery)

<partial name="_Scripts_JQuery" />

@Html.RenderScripts(Url, ResourceLocation.BeforeVendors)
@Html.RenderInlineScripts(Url, ResourceLocation.BeforeVendors)

<partial name="_Scripts_Bootstrap" />
<partial name="_Scripts_Validation" />
<partial name="_Scripts_Vendors_Admin" />

@Html.RenderScripts(Url, ResourceLocation.BeforeScripts)
@Html.RenderInlineScripts(Url, ResourceLocation.BeforeScripts)

<environment names="Development,Staging,ErrorTesting">
    <script src="~/hood/js/includes/globals.js"></script>
    <script src="~/hood/js/includes/stringhelpers.js"></script>
    <script src="~/hood/js/includes/uploader.js"></script>
    <script src="~/hood/js/includes/alerts.js"></script>
    <script src="~/hood/js/includes/helpers.js"></script>
    <script src="~/hood/js/includes/forms.js"></script>
    <script src="~/hood/js/includes/handlers.js"></script>
    <script src="~/hood/js/includes/datalist.js"></script>
    <script src="~/hood/js/includes/fileupload.js"></script>
    <script src="~/hood/js/includes/pager.js"></script>
    <script src="~/hood/js/includes/validator.js"></script>
    <script src="~/hood/js/includes/blades.js"></script>
    <script src="~/hood/js/includes/modals.js"></script>
    <script src="~/hood/js/includes/inline.js"></script>
    <script src="~/hood/js/includes/media.js"></script>
    <script src="~/hood/js/includes/users.js"></script>
    <script src="~/hood/js/includes/themes.js"></script>
    <script src="~/hood/js/includes/property.js"></script>
    <script src="~/hood/js/includes/subscriptions.js"></script>
    <script src="~/hood/js/includes/content.js"></script>
    <script src="~/hood/js/includes/forums.js"></script>
    <script src="~/hood/js/includes/logs.js"></script>
    <script src="~/hood/js/includes/google.js"></script>
    <script src="~/hood/js/admin.js"></script>
</environment>
<environment names="Production,PreProduction">
    <script async defer src="@string.Format("https://hood.azureedge.net/cdn/{0}/js/admin.packaged.js", _settings.GetVersion())" 
            asp-append-version="true"
            asp-fallback-test="$.hood.Admin"
            asp-fallback-src="~/hood/js/admin.packaged.js"></script>
</environment>
@if (_plugins.GoogleMapsApiKey.IsSet() && (_plugins.EnableGoogleMaps || _plugins.EnableGoogleGeocoding))
{
    <script async defer src="@string.Format("//maps.googleapis.com/maps/api/js?key={0}&callback=initGoogleMapsComplete{1}", _plugins.GoogleMapsApiKey, _plugins.EnableGoogleGeocoding ? "&libraries=places" : "")"></script>
}

@Html.RenderScripts(Url, ResourceLocation.AfterScripts)
@Html.RenderInlineScripts(Url, ResourceLocation.AfterScripts)

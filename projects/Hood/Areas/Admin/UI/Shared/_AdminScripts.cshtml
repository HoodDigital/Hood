@inject IThemesService _themes
@{
    IntegrationSettings _plugins = _settings.GetIntegrationSettings();
}
<script src="//code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>
<script src="~/lib/kendo-ui-core/js/kendo.ui.core.min.js"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
<script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js" asp-append-version="true"></script>
<script src="~/lib/hammer.js/hammer.js"></script>
<script src="~/lib/metisMenu/dist/metisMenu.js"></script>
<script src="~/lib/slimscroll/jquery.slimscroll.min.js"></script>
<script src="~/lib/dropzone/dist/min/dropzone.min.js"></script>
<script src="~/lib/bootstrap-datepicker/dist/js/bootstrap-datepicker.js"></script>
<script src="~/lib/bootstrap-colorpicker/dist/js/bootstrap-colorpicker.min.js"></script>
<script src="~/lib/tinymce/tinymce.min.js"></script>
<script src="~/lib/tinymce/themes/inlite/theme.min.js"></script>
<script src="~/lib/chart.js/dist/Chart.bundle.min.js"></script>
<script src="~/lib/counter-up/jquery.counterup.min.js"></script>
<script src="~/lib/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/lib/selectize/dist/js/standalone/selectize.min.js"></script>
<environment names="Development,Staging,ErrorTesting">
    <script src="~/lib/hood/src/js/includes/globals.js"></script>
    <script src="~/lib/hood/src/js/includes/stringhelpers.js"></script>
    <script src="~/lib/hood/src/js/includes/uploader.js"></script>
    <script src="~/lib/hood/src/js/includes/alerts.js"></script>
    <script src="~/lib/hood/src/js/includes/helpers.js"></script>
    <script src="~/lib/hood/src/js/includes/forms.js"></script>
    <script src="~/lib/hood/src/js/includes/handlers.js"></script>
    <script src="~/lib/hood/src/js/includes/datalist.js"></script>
    <script src="~/lib/hood/src/js/includes/fileupload.js"></script>
    <script src="~/lib/hood/src/js/includes/pager.js"></script>
    <script src="~/lib/hood/src/js/includes/validator.js"></script>
    <script src="~/lib/hood/src/js/includes/blades.js"></script>
    <script src="~/lib/hood/src/js/includes/modals.js"></script>
    <script src="~/lib/hood/src/js/includes/inline.js"></script>
    <script src="~/lib/hood/src/js/includes/media.js"></script>
    <script src="~/lib/hood/src/js/includes/users.js"></script>
    <script src="~/lib/hood/src/js/includes/themes.js"></script>
    <script src="~/lib/hood/src/js/includes/property.js"></script>
    <script src="~/lib/hood/src/js/includes/subscriptions.js"></script>
    <script src="~/lib/hood/src/js/includes/content.js"></script>
    <script src="~/lib/hood/src/js/includes/forums.js"></script>
    <script src="~/lib/hood/src/js/includes/google.js"></script>
    <script src="~/lib/hood/src/js/admin.js"></script>
</environment>
<environment names="Production,PreProduction">
    @if (_settings.GetIntegrationSettings().UseCDN)
    {
        <script src="@string.Format("//hood.azureedge.net/cdn/{0}/dist/js/admin.packaged.js", _settings.GetVersion())"></script>
    }
    else
    {
        <script src="~/lib/hood/dist/js/admin.packaged.js" asp-append-version="true"></script>
    }
</environment>
@if (_plugins.GoogleMapsApiKey.IsSet() && (_plugins.EnableGoogleMaps || _plugins.EnableGoogleGeocoding))
{
    <script async defer src="@string.Format("//maps.googleapis.com/maps/api/js?key={0}&callback=initGoogleMapsComplete{1}", _plugins.GoogleMapsApiKey, _plugins.EnableGoogleGeocoding ? "&libraries=places" : "")"></script>
}